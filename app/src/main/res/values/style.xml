        <!-- Customize your theme here -->
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryDark">@color/blue</item>
        <item name="toolbarStyle">@color/blue</item>
    </style>

    <style name="tab_text" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:fontFamily">@font/poppinsmedium</item>
        <item name="android:textColor">@color/blue</item>

    </style>

    <style name="title_appearance" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">20sp</item>
        <item name="android:fontFamily">@font/poppinsbold</item>
        <item name="android:textColor">#FFFFFFFF</item>
    </style>

</resources>